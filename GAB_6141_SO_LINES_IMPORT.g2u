	F.Intrinsic.Control.If(V.Odbc.con!rstInvoice.EOF,=,False)
		F.Intrinsic.String.Concat("Sales Order ",V.local.sOrderno," has been invoiced. Upload failed!",V.Local.sMsg)
		F.ODBC.Connection!con.Close 
