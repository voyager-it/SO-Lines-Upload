Program.Sub.CalcMargin.Start
V.Local.sOrderno.Declare
V.Local.sSQL.Declare
V.Local.sRet.Declare
V.Local.lCount.Declare
V.Local.sLines.Declare
V.Local.sCurLine.Declare
V.Local.fCost.Declare
V.Local.fPrice.Declare
V.Local.fMarPer.Declare
V.Local.fMarAmt.Declare

F.Intrinsic.UI.ChangeWaitStatus("Calculating Margin...")

F.Intrinsic.String.LPad(Variable.Passed.000003,0,7,V.local.sOrderno)

F.Intrinsic.String.Build("SELECT ORDER_NO, RECORD_NO, RECORD_TYPE, PART, PRICE, DISCOUNT_PRICE, COST, MARGIN FROM ORDER_LINES WHERE RECORD_TYPE = 'L' AND ORDER_NO = '{0}' ORDER BY RECORD_NO ASC;",V.Local.sOrderno,V.Local.sSQL)
F.ODBC.Connection!Con.ExecuteAndReturn(V.Local.sSQL,V.Local.sRet)

F.Intrinsic.String.Split(V.Local.sRet,"#$#",V.Local.sLines)

F.Intrinsic.Control.For(V.Local.lCount,0,V.Local.sLines.UBound,1)

	F.Intrinsic.UI.ChangeWaitStatus("Calculating Margin...",V.Local.lCount,0,V.Local.sLines.UBound)

	F.Intrinsic.String.Split(V.Local.sLines(V.Local.lCount),"*!*",V.Local.sCurLine)
	
	F.Intrinsic.String.Build("SELECT COST FROM INVENTORY_COST WHERE PART = '{0}'",V.Local.sCurLine(3),V.Local.sSQL)
	F.ODBC.Connection!Con.ExecuteAndReturn(V.Local.sSQL,V.Local.fCost)
	
	F.Intrinsic.Control.If(V.Local.sCurLine(5),=,0)
		V.Local.fMarPer.Set(0)
	F.Intrinsic.Control.Else
		V.Local.fPrice.Set(V.Local.sCurLine(5))
		F.Intrinsic.Math.Sub(V.Local.fPrice,V.Local.fCost,V.Local.fMarAmt)
		F.Intrinsic.Math.Div(V.Local.fMarAmt,V.Local.fPrice,V.Local.fMarPer)
		F.Intrinsic.Math.Mult(V.Local.fMarPer,100,V.Local.fMarPer)
		F.Intrinsic.Math.Round(V.Local.fMarPer,4,V.Local.fMarPer)
	F.Intrinsic.Control.EndIf
	
	F.Intrinsic.String.Build("UPDATE ORDER_LINES SET COST = '{0}', MARGIN = '{1}' WHERE ORDER_NO = '{2}' AND RECORD_NO = '{3}'",V.Local.fCost,V.Local.fMarPer,V.Local.sOrderno,V.Local.sCurLine(1),V.Local.sSQL)
	F.ODBC.Connection!Con.Execute(V.Local.sSQL)

F.Intrinsic.Control.Next(V.Local.lCount)

Program.Sub.CalcMargin.End

